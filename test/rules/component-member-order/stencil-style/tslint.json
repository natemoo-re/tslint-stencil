{
  "rulesDirectory": ["../../../../rules"],
  "rules": {
    "component-member-order": [
      true,
      {
        "order": [
          "own-prop",
          "element",
          "state",
          "watched-state",
          "internal-prop",
          "prop",
          "watched-prop",
          "event",
          "lifecycle",
          "listen",
          "method",
          "own-method",
          "stencil-method"
        ],
        "watch-follows-prop": true,
        "alphabetical": true
      }
    ]
  }
}
